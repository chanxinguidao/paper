<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>测试</title>
    <meta name="decorator" content="ani"/>
</head>
<body>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        专家指派筛选条件
                    </h3>
                </div>
                <div style="height: 260px" class="panel-body">
                    <form id="myform" class="form-horizontal">
                        <div style="margin-top: 30px" class="form-group">
                            <label class="col-sm-2 control-label">专家高校：</label>
                            <div class="col-sm-10">
                                <input class="i-checks" name="dsjb" type="checkbox" value="211">211高校
                                <input class="i-checks" name="dsjb" type="checkbox" value="985">985高校
                            </div>
                        </div>
                        <div style="margin-top: 30px" class="form-group">
                            <label class="col-sm-2 control-label">专家类型：</label>
                            <div class="col-sm-10">
                                <input class="i-checks" name="ZJXLDM" type="radio" value="0">高校专家
                                <input class="i-checks" name="ZJXLDM" type="radio" value="1">行业专家
                            </div>
                        </div>
                        <div style="margin-top: 30px" class="form-group">
                            <label class="col-sm-2 control-label">筛选原则：</label>
                            <div class="col-sm-10">
                                <input class="i-checks" name="gxc" type="checkbox" value="1">高职称优先
                                <input class="i-checks" name="gxl" type="checkbox" value="1">高学历优先
                                <input checked class="i-checks" name="page.orderBy" type="checkbox" value="pscs desc">专家评审次数多的优先
                                <input checked class="i-checks" name="jhyz" type="checkbox" value="1">均衡原则
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>
