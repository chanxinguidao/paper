<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqu.modules.database.costc.mapper.CostcMapper">
    
	<sql id="costcColumns">
		a.id AS "id",
		a.create_by AS "createBy.id",
		a.create_date AS "createDate",
		a.update_by AS "updateBy.id",
		a.update_date AS "updateDate",
		a.del_flag AS "delFlag",
		a.gxdm AS "gxdm",
		a.zydm AS "zydm",
		a.psfy AS "psfy"
	</sql>
	
	<sql id="costcJoins">
		
	</sql>
	
    
	<select id="get" resultType="Costc" >
		SELECT 
			<include refid="costcColumns"/>
		FROM t_cost_c a
		<include refid="costcJoins"/>
		WHERE a.id = #{id}
	</select>
	
	<select id="findList" resultType="Costc" >
		SELECT 
			<include refid="costcColumns"/>
		FROM t_cost_c a
		<include refid="costcJoins"/>
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			${dataScope}
			<if test="gxdm != null and gxdm != ''">
				AND a.gxdm = #{gxdm}
			</if>
			<if test="zydm != null and zydm != ''">
				AND a.zydm = #{zydm}
			</if>
			<if test="psfy != null and psfy != ''">
				AND a.psfy = #{psfy}
			</if>
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.update_date DESC
			</otherwise>
		</choose>
	</select>
	
	<select id="findAllList" resultType="Costc" >
		SELECT 
			<include refid="costcColumns"/>
		FROM t_cost_c a
		<include refid="costcJoins"/>
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			${dataScope}
		</where>		
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.update_date DESC
			</otherwise>
		</choose>
	</select>
	
	<insert id="insert">
		INSERT INTO t_cost_c(
			id,
			create_by,
			create_date,
			update_by,
			update_date,
			del_flag,
			gxdm,
			zydm,
			psfy
		) VALUES (
			#{id},
			#{createBy.id},
			#{createDate},
			#{updateBy.id},
			#{updateDate},
			#{delFlag},
			#{gxdm},
			#{zydm},
			#{psfy}
		)
	</insert>
	
	<update id="update">
		UPDATE t_cost_c SET 	
			update_by = #{updateBy.id},
			update_date = #{updateDate},
			gxdm = #{gxdm},
			zydm = #{zydm},
			psfy = #{psfy}
		WHERE id = #{id}
	</update>
	
	
	<!--物理删除-->
	<update id="delete">
		DELETE FROM t_cost_c
		WHERE id = #{id}
	</update>
	
	<!--逻辑删除-->
	<update id="deleteByLogic">
		UPDATE t_cost_c SET 
			del_flag = #{DEL_FLAG_DELETE}
		WHERE id = #{id}
	</update>
	
	
	<!-- 根据实体名称和字段名称和字段值获取唯一记录 -->
	<select id="findUniqueByProperty" resultType="Costc" statementType="STATEMENT">
		select * FROM t_cost_c  where ${propertyName} = '${value}'
	</select>
	
</mapper>